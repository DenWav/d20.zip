        "format": "prettier --write ."
    },
    "dependencies": {
        "@dimforge/rapier3d-compat": "^0.11.2",
        "stats.js": "^0.17.0",
        "three": "^0.182.0"
    },
    "devDependencies": {
        "@types/three": "^0.182.0",
        "concurrently": "^9.2.1",
        "esbuild": "^0.27.2",
        "live-server": "^1.2.2",
        "prettier": "^3.8.1",
        "typescript": "^5.5.3"
    },
    "private": true
}
