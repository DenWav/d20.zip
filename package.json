{
    "name": "d20.zip",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "compile": "tsc",
        "build": "tsc && esbuild src/vendor.ts --bundle --minify --sourcemap --format=esm --outfile=dist/vendor.js",
